# Database (Neon Postgres)
DATABASE_URL=postgres://user:password@host/database

# AI Provider Selection (optional - defaults to auto-detect)
# Options: clawdbot, openai, anthropic, ollama
AI_PROVIDER=

# Clawdbot Gateway (local AI with memory, tools, multi-agent)
CLAWDBOT_GATEWAY_URL=http://127.0.0.1:18789
CLAWDBOT_GATEWAY_TOKEN=your_token_here
CLAWDBOT_AGENT_ID=main

# OpenAI (fallback if Clawdbot not available)
OPENAI_API_KEY=

# Anthropic (fallback)
ANTHROPIC_API_KEY=
